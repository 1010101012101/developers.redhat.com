<div class="layout-container katacoda-courses">
        <header role="banner">
          <div class="rhd-masthead">
            <div class="container">
              <div class="rhd-masthead-inner">
              
                {{ page.rhd_katacoda_header }}
                {{ page.rhd_katacoda_mobile_tray }}
                <div class="menu-toggle-wrapper">
                  <button class="menu-toggle">Menu<span class="icon"><i class="far fa-bars"></i></span></button>
                </div>
              </div>
            </div>
          </div>
        </header>
        <div class="rhd-mobile-overlay"></div>
        <div class="rhd-search-toggle-overlay"></div>
      
        <main role="main">
          <a id="main-content" tabindex="-1"></a>{# link is in html.html.twig #}
      
          <div class="layout-content">
            {{ page.content }}
          </div>{# /.layout-content #}
        </main>
      
      
      </div>{# /.layout-container #}